<!DOCTYPE html>
<html>
<head>
  <title>Edughana</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if user_signed_in? %>
  	<%= render :partial => "layouts/common/header" %><br>
  <% else %>
  <% end %>
  <div id="div_flash_message" class="container" >
  	<%= render :partial=>"/layouts/common/flash_messages" -%>
  </div>
  <%= yield %>
</body>
</html>
